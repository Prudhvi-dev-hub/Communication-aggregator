generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model MessageRequest {
  id          String   @id @default(uuid())
  requestId   String   @unique
  channel     String
  to          String
  subject     String?
  body        String?
  createdAt   DateTime @default(now())
}
